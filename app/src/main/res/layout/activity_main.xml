<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.kinga.calculatorrpn.MainActivity">

    <GridLayout
        android:id="@+id/gridLayout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout">

        <Button
            android:id="@+id/button7"
            android:layout_width="70dp"
            android:layout_height="wrap_content"
            android:text="7" />

        <Button
            android:id="@+id/button8"
            android:layout_width="70dp"
            android:layout_height="wrap_content"
            android:layout_column="1"
            android:layout_row="0"
            android:text="8" />

        <Button
            android:id="@+id/button9"
            android:layout_width="70dp"
            android:layout_height="wrap_content"
            android:layout_column="2"
            android:layout_row="0"
            android:text="9" />

        <Button
            android:id="@+id/buttonDivide"
            android:layout_width="70dp"
            android:layout_height="wrap_content"
            android:layout_column="3"
            android:layout_row="0"
            android:text="/" />

        <Button
            android:id="@+id/button4"
            android:layout_width="70dp"
            android:layout_height="wrap_content"
            android:layout_column="0"
            android:layout_row="1"
            android:text="4" />

        <Button
            android:id="@+id/button5"
            android:layout_width="70dp"
            android:layout_height="wrap_content"
            android:layout_column="1"
            android:layout_row="1"
            android:text="5" />

        <Button
            android:id="@+id/button6"
            android:layout_width="70dp"
            android:layout_height="wrap_content"
            android:layout_column="2"
            android:layout_row="1"
            android:text="6" />

        <Button
            android:id="@+id/buttonMultiply"
            android:layout_width="70dp"
            android:layout_height="wrap_content"
            android:layout_column="3"
            android:layout_row="1"
            android:text="*" />

        <Button
            android:id="@+id/button1"
            android:layout_width="70dp"
            android:layout_height="wrap_content"
            android:layout_column="0"
            android:layout_row="2"
            android:text="1" />

        <Button
            android:id="@+id/button2"
            android:layout_width="70dp"
            android:layout_height="wrap_content"
            android:layout_column="1"
            android:layout_row="2"
            android:text="2" />

        <Button
            android:id="@+id/button3"
            android:layout_width="70dp"
            android:layout_height="wrap_content"
            android:layout_column="2"
            android:layout_row="2"
            android:text="3" />

        <Button
            android:id="@+id/buttonPlus"
            android:layout_width="70dp"
            android:layout_height="wrap_content"
            android:layout_column="3"
            android:layout_row="2"
            android:text="+" />

        <Button
            android:id="@+id/button0"
            android:layout_width="70dp"
            android:layout_height="wrap_content"
            android:layout_column="0"
            android:layout_row="3"
            android:text="0" />

        <Button
            android:id="@+id/buttonDot"
            android:layout_width="70dp"
            android:layout_height="wrap_content"
            android:layout_column="1"
            android:layout_row="3"
            android:text="." />

        <Button
            android:id="@+id/buttonChangeSign"
            android:layout_width="70dp"
            android:layout_height="wrap_content"
            android:layout_column="2"
            android:layout_row="3"
            android:text="+/-" />

        <Button
            android:id="@+id/buttonMinus"
            android:layout_width="70dp"
            android:layout_height="wrap_content"
            android:layout_column="3"
            android:layout_row="3"
            android:text="-" />

        <Button
            android:id="@+id/buttonUndo"
            android:layout_width="70dp"
            android:layout_height="wrap_content"
            android:layout_column="4"
            android:layout_row="0"
            android:text="UNDO" />

        <Button
            android:id="@+id/buttonExp"
            android:layout_width="70dp"
            android:layout_height="wrap_content"
            android:layout_column="4"
            android:layout_row="1"
            android:text="EXP" />

        <Button
            android:id="@+id/buttonSqrt"
            android:layout_width="70dp"
            android:layout_height="wrap_content"
            android:layout_column="4"
            android:layout_row="2"
            android:text="sqrt" />

        <Button
            android:id="@+id/buttonPercent"
            android:layout_width="70dp"
            android:layout_height="wrap_content"
            android:layout_column="4"
            android:layout_row="3"
            android:text="%" />

        <Button
            android:id="@+id/buttonEnter"
            android:layout_width="70dp"
            android:layout_height="wrap_content"
            android:layout_column="0"
            android:layout_row="4"
            android:text="enter" />

        <Button
            android:id="@+id/buttonSwap"
            android:layout_width="70dp"
            android:layout_height="wrap_content"
            android:layout_column="1"
            android:layout_row="4"
            android:text="swap" />

        <Button
            android:id="@+id/buttonDrop"
            android:layout_width="70dp"
            android:layout_height="wrap_content"
            android:layout_column="2"
            android:layout_row="4"
            android:text="drop" />

        <Button
            android:id="@+id/buttonAC"
            android:layout_width="70dp"
            android:layout_height="wrap_content"
            android:layout_column="3"
            android:layout_row="4"
            android:text="AC" />

        <Button
            android:id="@+id/buttonSettings"
            android:layout_width="70dp"
            android:layout_height="wrap_content"
            android:layout_column="4"
            android:layout_row="4"
            android:text="SETT" />

    </GridLayout>

    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="361dp"
        android:layout_height="0dp"
        android:layout_marginBottom="34dp"
        android:layout_marginTop="42dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toTopOf="@+id/gridLayout"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.571"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">


        <TextView
            android:id="@+id/stack3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="TextView"
            android:textSize="30dp" />

        <TextView
            android:id="@+id/stack2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="TextView"
            android:textSize="30dp" />

        <TextView
            android:id="@+id/stack1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="TextView"
            android:textSize="30dp" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="TextView"
            android:textSize="30dp" />
    </LinearLayout>
</android.support.constraint.ConstraintLayout>
